{
  "name": "js-sort",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "src/*",
    "src/**/*"
  ],
  "scripts": {
    "build:i": "parcel build index.ts -d dist/",
    "build:a": "parcel build src/algorithm/* -d dist/algorithm",
    "build:p": "parcel build src/poisoning/* -d dist/poisoning",
    "build": "yarn build:i && yarn build:a && yarn build:p"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "typescript": "^3.7.4"
  },
  "dependencies": {}
}
